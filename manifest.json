{
    "manifest_version": 3,
    "name": "Qırımtatarca turn on for alerts.in.ua",
    "version": "0.4",
    "description": "Turns on Crimean Tatar for alerts.in.ua",
    "permissions": ["https://alerts.in.ua/"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon/16x16.png",
            "48": "icon/48x48.png",
            "128": "icon/128x128.png"
        }
    },
    "icons": {
        "16": "icon/16x16.png",
        "48": "icon/48x48.png",
        "128": "icon/128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://alerts.in.ua/"],
            "js": ["script.js"]
        }
    ]
}