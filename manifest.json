{
    "manifest_version": 3,
    "name": "Qırımtatarca turn on for alerts.in.ua",
    "version": "0.7",
    "description": "Turns on Crimean Tatar for alerts.in.ua",
    "permissions": ["https://alerts.in.ua/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon/16.png",
            "48": "icon/48.png",
            "128": "icon/128.png",
            "256": "icon/256.png",
            "512": "icon/512.png"
        }
    },
    "icons": {
        "16": "icon/16.png",
        "48": "icon/48.png",
        "128": "icon/128.png",
        "256": "icon/256.png",
        "512": "icon/512.png"
    },
    "content_scripts": [
        {
            "matches": ["https://alerts.in.ua/*"],
            "js": ["script.js"]
        }
    ]
}